  "title": "Prerequisite Relationship",
  "description": "A Prerequisite Relationship lists prerequisites and their version ranges.",
  "type": "object",
  "minProperties": 1,
  "propertyNames": { "$ref": "term.schema.json" },
  "additionalProperties": { "$ref": "version_range.schema.json" },
  "examples": [
    {
      "PostgreSQL": "8.0.0",
      "PostGIS": "1.5.0"
    },
    {
      "PostgreSQL": "8.4.0"
    },
    {
      "sha1": 0
    }
  ]
}
